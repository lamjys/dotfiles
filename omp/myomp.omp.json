{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "prefix": "",
            "template": "{{ .UserName }}"
          },
          "foreground": "#7E46B6",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": " ",
            "prefix": ""
          },
          "foreground": "#DD4814",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "prefix": "",
            "template": "{{ .ComputerName }}"
          },
          "foreground": "#4169E1",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "prefix": "",
            "style": "full"
          },
          "foreground": "#87FF00",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "display_stash_count": true,
            "display_status": true,
            "display_upstream_icon": true,
            "fetch_status": true,
            "template": "{{ .HEAD }}{{ .BranchStatus }}{{ if .Staging.Changed }}<#87FF00> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> \u25cf {{ .Working.String }}</>{{ end }}"
          },
          "foreground": "#5FD7FF",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "prefix": "",
            "text": "âš½"
          },
          "foreground": "#FFFFFF",
          "type": "text",
          "style": "plain"
        },
        {
          "template": " \ue235 {{ .Full }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#906cff",
          "type": "python",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3
}
